---
import Layout from "../layouts/Layout.astro";
import Nav from "../components/Nav.astro";
import Proyecto from "../components/Proyecto.astro";
import data from "../proyectsInfo";
import Footer from "../components/Footer.astro";
import "../styles/global.css"

const bookhub = data[0];
const meteolocal = data[1];
const webcam = data[2];
const twitter = data[3];
const gamecoreAndroid = data[4];
const cvmaker = data[5]
---

<Layout title="Samuel Amat - Full Stack Developer">
	<Nav />
    <div class="fadeIn md:fadeInLeft">
      <Proyecto comp={bookhub.imgComp} phone={bookhub.imgPhone} nombreProyecto={bookhub.nombre} descripcion={bookhub.descripcionEs} tecnologias={bookhub.tecnologias} liveDemo={bookhub.liveDemoUrl} repositorio={bookhub.repoUrl}/>
    </div>

    <div class="fadeIn md:fadeInRight">
      <Proyecto comp={meteolocal.imgComp} phone={meteolocal.imgPhone} nombreProyecto={meteolocal.nombre} descripcion={meteolocal.descripcionEs} tecnologias={meteolocal.tecnologias} liveDemo={meteolocal.liveDemoUrl} repositorio={meteolocal.repoUrl}/>
    </div>
    
    <div class="fadeIn md:fadeInLeft">
      <Proyecto comp={webcam.imgComp} nombreProyecto={webcam.nombre} descripcion={webcam.descripcionEs} tecnologias={webcam.tecnologias} repositorio={webcam.repoUrl}/>
    </div>
    
    <div class="fadeIn md:fadeInRight">
      <Proyecto comp={twitter.imgComp} phone={twitter.imgPhone} nombreProyecto={twitter.nombre} descripcion={twitter.descripcionEs} tecnologias={twitter.tecnologias} liveDemo={twitter.liveDemoUrl} repositorio={twitter.repoUrl}/>
    </div>
    
    <div class="fadeIn md:fadeInLeft">
      <Proyecto comp={gamecoreAndroid.imgComp} phone={gamecoreAndroid.imgPhone} nombreProyecto={gamecoreAndroid.nombre} descripcion={gamecoreAndroid.descripcionEs} tecnologias={gamecoreAndroid.tecnologias} liveDemo={gamecoreAndroid.liveDemoUrl} repositorio={gamecoreAndroid.repoUrl}/>
    </div>

    <div class="fadeIn md:fadeInRight">
      <Proyecto comp={cvmaker.imgComp} nombreProyecto={cvmaker.nombre} descripcion={cvmaker.descripcionEs} tecnologias={cvmaker.tecnologias} liveDemo={cvmaker.liveDemoUrl} repositorio={cvmaker.repoUrl}/>
    </div>

    <Footer />
</Layout>